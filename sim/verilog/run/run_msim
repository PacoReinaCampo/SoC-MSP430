#!/bin/bash
export PATH=$PATH:/opt/intelFPGA_pro/19.2/modelsim_ase/linuxaloem/

# Enable/Disable waveform dumping
OMSP_NODUMP=0
export OMSP_NODUMP

# Choose simulator:
#                   - iverilog  : Icarus Verilog  (default)
#                   - cver      : CVer
#                   - verilog   : Verilog-XL
#                   - ncverilog : NC-Verilog
#                   - vcs       : VCS
#                   - msim      : Modelsim
#                   - isim      : Xilinx simulator
OMSP_SIMULATOR=msim
export OMSP_SIMULATOR

../bin/msp430sim leds
